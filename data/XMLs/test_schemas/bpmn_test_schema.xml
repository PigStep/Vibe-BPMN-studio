      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="BPMNEdge_flow9" bpmnElement="flow9">
        <di:waypoint x="100" y="665" />
        <di:waypoint x="100" y="420" />
      </bpmndi:BPMNEdge>
