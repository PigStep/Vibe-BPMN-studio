      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="BPMNEdge_flow7" bpmnElement="flow7">
        <di:waypoint x="100" y="610" />
        <di:waypoint x="100" y="640" />
      </bpmndi:BPMNEdge>
