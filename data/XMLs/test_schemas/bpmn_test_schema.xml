<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                  id="logistics_product_launch"
                  targetNamespace="http://bpmn.io/schema/bpmn">
  <bpmn:collaboration id="Collaboration_1">
    <bpmn:participant id="market_research_team" name="Market Research Team" processRef="Process_1" />
    <bpmn:participant id="feasibility_analyst" name="Feasibility Analyst" processRef="Process_1" />
    <bpmn:participant id="product_design_team" name="Product Design Team" processRef="Process_1" />
    <bpmn:participant id="prototype_developer" name="Prototype Developer" processRef="Process_1" />
    <bpmn:participant id="supplier_manager" name="Supplier Manager" processRef="Process_1" />
    <bpmn:participant id="production_team" name="Production Team" processRef="Process_1" />
    <bpmn:participant id="marketing_team" name="Marketing Team" processRef="Process_1" />
    <bpmn:participant id="distribution_manager" name="Distribution Manager" processRef="Process_1" />
    <bpmn:participant id="launch_coordinator" name="Launch Coordinator" processRef="Process_1" />
  </bpmn:collaboration>

  <bpmn:process id="Process_1" isExecutable="false">
    <bpmn:startEvent id="start" name="Product Idea Initiation">
      <bpmn:outgoing>flow1</bpmn:outgoing>
    </bpmn:startEvent>

    <bpmn:task id="task1" name="Market Research">
      <bpmn:incoming>flow1</bpmn:incoming>
      <bpmn:outgoing>flow2</bpmn:outgoing>
    </bpmn:task>

    <bpmn:userTask id="task2" name="Feasibility Study">
      <bpmn:incoming>flow2</bpmn:incoming>
      <bpmn:outgoing>flow3</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:exclusiveGateway id="gateway1" name="Feasibility Check">
      <bpmn:incoming>flow3</bpmn:incoming>
      <bpmn:outgoing>flow4</bpmn:outgoing>
      <bpmn:outgoing>flow5</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:task id="task3" name="Product Design">
      <bpmn:incoming>flow4</bpmn:incoming>
      <bpmn:outgoing>flow6</bpmn:outgoing>
    </bpmn:task>

    <bpmn:userTask id="task4" name="Prototype Development">
      <bpmn:incoming>flow6</bpmn:incoming>
      <bpmn:outgoing>flow7</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:serviceTask id="task5" name="Supplier Selection">
      <bpmn:incoming>flow8</bpmn:incoming>
      <bpmn:outgoing>flow10</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:exclusiveGateway id="gateway2" name="Prototype Approval">
      <bpmn:incoming>flow7</bpmn:incoming>
      <bpmn:outgoing>flow8</bpmn:outgoing>
      <bpmn:outgoing>flow9</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:task id="task6" name="Mass Production">
      <bpmn:incoming>flow10</bpmn:incoming>
      <bpmn:outgoing>flow11</bpmn:outgoing>
    </bpmn:task>

    <bpmn:userTask id="task7" name="Marketing Campaign">
      <bpmn:incoming>flow11</bpmn:incoming>
      <bpmn:outgoing>flow12</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:serviceTask id="task8" name="Distribution Planning">
      <bpmn:incoming>flow12</bpmn:incoming>
      <bpmn:outgoing>flow13</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:task id="task9" name="Product Launch">
      <bpmn:incoming>flow13</bpmn:incoming>
      <bpmn:outgoing>flow14</bpmn:outgoing>
    </bpmn:task>

    <bpmn:endEvent id="end" name="Product Launch Complete">
      <bpmn:incoming>flow5</bpmn:incoming>
      <bpmn:incoming>flow14</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:sequenceFlow id="flow1" sourceRef="start" targetRef="task1" />
    <bpmn:sequenceFlow id="flow2" sourceRef="task1" targetRef="task2" />
    <bpmn:sequenceFlow id="flow3" sourceRef="task2" targetRef="gateway1" />
    <bpmn:sequenceFlow id="flow4" sourceRef="gateway1" targetRef="task3" name="Feasible" />
    <bpmn:sequenceFlow id="flow5" sourceRef="gateway1" targetRef="end" name="Not Feasible" />
    <bpmn:sequenceFlow id="flow6" sourceRef="task3" targetRef="task4" />
    <bpmn:sequenceFlow id="flow7" sourceRef="task4" targetRef="gateway2" />
    <bpmn:sequenceFlow id="flow8" sourceRef="gateway2" targetRef="task5" name="Approved" />
    <bpmn:sequenceFlow id="flow9" sourceRef="gateway2" targetRef="task3" name="Needs Revision" />
    <bpmn:sequenceFlow id="flow10" sourceRef="task5" targetRef="task6" />
    <bpmn:sequenceFlow id="flow11" sourceRef="task6" targetRef="task7" />
    <bpmn:sequenceFlow id="flow12" sourceRef="task7" targetRef="task8" />
    <bpmn:sequenceFlow id="flow13" sourceRef="task8" targetRef="task9" />
    <bpmn:sequenceFlow id="flow14" sourceRef="task9" targetRef="end" />
  </bpmn:process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="Process_1">
      <bpmndi:BPMNShape id="BPMNShape_start" bpmnElement="start">
        <dc:Bounds x="50" y="50" width="36" height="36" />
      </bpmndi:BPMNShape>
